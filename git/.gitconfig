[user]
	name = Bruno Brunkow
	email = self@brunobrunkow.de
[core]
	excludesfile = /Users/brunobrunkow/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/brunobrunkow/.stCommitMsg
[push]
	autoSetupRemote = true
